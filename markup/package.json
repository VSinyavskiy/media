{
  "name": "markup",
  "version": "3.6.0",
  "description": "markup",
  "repository": {},
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "better-npm-run start || true",
    "build:dev": "better-npm-run build:dev && better-npm-run beautify || true",
    "watch:dev": "better-npm-run watch:dev || true",
    "build:prod": "better-npm-run build:prod && better-npm-run beautify || true",
    "watch:prod": "better-npm-run watch:prod || true",
    "beautify": "better-npm-run beautify || true"
  },
  "betterScripts": {
    "beautify": {
      "command": "node ./config/beautify.js"
    },
    "start": {
      "command": "webpack-dev-server --config webpack.config.js --colors",
      "env": {
        "NODE_ENV": "dev-server"
      }
    },
    "build:dev": {
      "command": "webpack --config webpack.config.js --progress --colors",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "watch:dev": {
      "command": "webpack --config webpack.config.js --colors --watch",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "webpack --config webpack.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "watch:prod": {
      "command": "webpack --config webpack.config.js --colors --watch",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "@glidejs/glide": "^3.2.3",
    "@mdi/font": "^2.5.94",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bourbon": "^5.1.0",
    "clipboard": "^2.0.1",
    "core-js": "^2.5.7",
    "delegated-events": "^1.0.9",
    "dom4": "^2.1.3",
    "inputmask": "^4.0.0",
    "jquery": "^3.2.1",
    "normalize.css": "^8.0.0",
    "parallax-js": "^3.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.0.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.24.1",
    "better-npm-run": "0.1.0",
    "bundle-loader": "^0.5.5",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.1.5",
    "js-beautify": "^1.7.5",
    "node-sass": "^4.5.3",
    "perfectionist": "^2.4.0",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^2.0.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^7.0.3",
    "script-loader": "^0.7.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-merge": "^4.1.0"
  }
}
